{
  "schemaVersion": 1,
  "id": "hashicraft-bot",
  "version": "${version}",

  "name": "HashiCraft Bot",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Nic Jackson",
    "ErikVeld"
  ],
  "contact": {
    "homepage": "https://hashicraft.com",
    "sources": "https://github.com/HashiCraft/minecraft-fabric-mod-hashicraft-bot"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
